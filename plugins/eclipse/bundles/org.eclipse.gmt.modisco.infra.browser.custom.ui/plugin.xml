<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.gmt.modisco"
            name="%modisco.category.name">
      </category>
      <view
            category="org.eclipse.gmt.modisco"
            class="org.eclipse.gmt.modisco.infra.browser.custom.ui.views.CustomizationsView"
            icon="icons/uiCustom.gif"
            id="org.eclipse.gmt.modisco.infra.browser.custom.view"
            name="%customizations.view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.eclipse.gmt.modisco"
            class="org.eclipse.gmt.modisco.infra.browser.custom.ui.wizards.NewCustomizationWizard"
            hasPages="true"
            icon="icons/modisco_new_customization.gif"
            id="org.eclipse.gmt.modisco.infra.ui.browser.custom.ui.newCustomizationWizard"
            name="%newCustomization.wizard.name"
            project="false">
      </wizard>
   </extension>
</plugin>
