	</session-config>
 
 
	<!-- TEST Tag Library Descriptors -->
	<taglib>
		<taglib-uri>taglib-uri-example</taglib-uri>
		<taglib-location>taglib-location-example</taglib-location>
	</taglib>
 
 
 	<!-- TEST Modisco welcome-file-list-->
	<welcome-file-list>  
		<welcome-file>welcome-file-example</welcome-file>
	</welcome-file-list>
	
</web-app>
