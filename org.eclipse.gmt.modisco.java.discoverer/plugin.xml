<plugin>
   <extension
         point="org.eclipse.gmt.modisco.discoverers">
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.DiscoverJavaModelFromJavaProject"
            icon="icons/java_model_obj.gif"
            name="%discoverer.javaProject2Model">
      </discoverer>
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.DiscoverKdmModelFromJavaProject"
            icon="icons/kdmsdk_16.gif"
            name="%discoverer.javaProject2KDM">
      </discoverer>
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.DiscoverJavaModelFromClassFile"
            icon="icons/java_model_obj.gif"
            name="%discoverer.classFile2Model">
      </discoverer>
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.DiscoverJavaModelFromLibrary"
            icon="icons/java_model_obj.gif"
            name="%discoverer.library2Model">
      </discoverer>
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.DiscoverKdmModelFromJavaModel"
            icon="icons/kdmsdk_16.gif"
            name="%discoverer.model2KDM">
      </discoverer>
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.BrowseJavaCode"
            icon="icons/link-with-editor.gif"
            name="%discoverer.name">
      </discoverer>
      <discoverer
            class="org.eclipse.gmt.modisco.java.actions.BrowseJavaModel"
            icon="icons/link-with-editor.gif"
            name="%discoverer.name.0">
      </discoverer>
   </extension>
</plugin>
